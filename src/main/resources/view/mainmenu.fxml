      </ImageView>
      <ImageView fx:id="imgOptions" fitHeight="82.0" fitWidth="562.0" layoutX="359.0" layoutY="402.0" onMouseClicked="#onOptionsClick" pickOnBounds="true" preserveRatio="true">
